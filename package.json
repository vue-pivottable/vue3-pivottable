    "typecheck": "tsc --noEmit",
    "build:all": "pnpm clean && pnpm build && pnpm -r --filter './packages/*' build",
    "dev:all": "concurrently \"pnpm dev\" \"pnpm -r --parallel run dev\"",
    "prepare": "husky"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
